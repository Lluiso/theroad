  StoppingDialogues:
  - Dialogue:
    - CharacterName: Carol
      Message: Oh dear, I wouldn't trust her sorts...
      MessageBoxType: 0
    Conditions:
      CharacterBeingPickedUp:
      - Salima
  LeaveCarDialogue:
  - CharacterName: Carol
    Message: Please, wait!
    MessageBoxType: 0
  - CharacterName: Carol
    Message: The old lady tries to run after the car as you speed off.
    MessageBoxType: 3
  - CharacterName: Carol
    Message: You look in your rear view mirror and see fall.
    MessageBoxType: 3
  - CharacterName: Carol
    Message: She does not get back up.
    MessageBoxType: 3
